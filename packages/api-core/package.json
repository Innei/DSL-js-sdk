{
  "name": "@rss3/api-core",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./decentralized": {
      "types": "./dist/esm/decentralized.d.ts",
      "import": "./dist/esm/decentralized.js",
      "require": "./dist/cjs/decentralized.js"
    },
    "./dsl": {
      "types": "./dist/esm/dsl.d.ts",
      "import": "./dist/esm/dsl.js",
      "require": "./dist/cjs/dsl.js"
    },
    "./rss": {
      "types": "./dist/esm/rss.d.ts",
      "import": "./dist/esm/rss.js",
      "require": "./dist/cjs/rss.js"
    },
    "./bridge": {
      "types": "./dist/esm/bridge.d.ts",
      "import": "./dist/esm/bridge.js",
      "require": "./dist/cjs/bridge.js"
    },
    "./nta": {
      "types": "./dist/esm/nta.d.ts",
      "import": "./dist/esm/nta.js",
      "require": "./dist/cjs/nta.js"
    },
    "./stake": {
      "types": "./dist/esm/stake.d.ts",
      "import": "./dist/esm/stake.js",
      "require": "./dist/cjs/stake.js"
    },
    "./chips": {
      "types": "./dist/esm/chips.d.ts",
      "import": "./dist/esm/chips.js",
      "require": "./dist/cjs/chips.js"
    },
    "./snapshots": {
      "types": "./dist/esm/snapshots.d.ts",
      "import": "./dist/esm/snapshots.js",
      "require": "./dist/cjs/snapshots.js"
    },
    "./node": {
      "types": "./dist/esm/node.d.ts",
      "import": "./dist/esm/node.js",
      "require": "./dist/cjs/node.js"
    },
    "./epoch": {
      "types": "./dist/esm/epoch.d.ts",
      "import": "./dist/esm/epoch.js",
      "require": "./dist/cjs/epoch.js"
    },
    "./networks": {
      "types": "./dist/esm/networks.d.ts",
      "import": "./dist/esm/networks.js",
      "require": "./dist/cjs/networks.js"
    }
  },
  "scripts": {
    "build": "concurrently 'tsc -p tsconfig.cjs.json' 'tsc -p tsconfig.esm.json'",
    "prebuild": "rm -rf dist",
    "download-openapi-json": "tsx scripts/download-openapi-json",
    "generate-openapi-schema": "tsx scripts/generate-openapi-schema.ts",
    "categorize-requests": "tsx scripts/categorize-requests.ts"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "map-obj": "^5.0.2",
    "openapi-typescript": "^7.0.0-rc.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "openapi-fetch": "^0.9.7",
    "ts-case-convert": "^2.0.7"
  }
}
