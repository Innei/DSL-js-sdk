{
  "name": "@rss3/api-core",
  "version": "0.0.8",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./client": {
      "types": "./dist/esm/client.d.ts",
      "import": "./dist/esm/client.js",
      "default": "./dist/cjs/client.js"
    },
    "./decentralized": {
      "types": "./dist/esm/decentralized.d.ts",
      "import": "./dist/esm/decentralized.js",
      "default": "./dist/cjs/decentralized.js"
    },
    "./dsl": {
      "types": "./dist/esm/dsl.d.ts",
      "import": "./dist/esm/dsl.js",
      "default": "./dist/cjs/dsl.js"
    },
    "./rss": {
      "types": "./dist/esm/rss.d.ts",
      "import": "./dist/esm/rss.js",
      "default": "./dist/cjs/rss.js"
    },
    "./bridge": {
      "types": "./dist/esm/bridge.d.ts",
      "import": "./dist/esm/bridge.js",
      "default": "./dist/cjs/bridge.js"
    },
    "./nta": {
      "types": "./dist/esm/nta.d.ts",
      "import": "./dist/esm/nta.js",
      "default": "./dist/cjs/nta.js"
    },
    "./stake": {
      "types": "./dist/esm/stake.d.ts",
      "import": "./dist/esm/stake.js",
      "default": "./dist/cjs/stake.js"
    },
    "./chips": {
      "types": "./dist/esm/chips.d.ts",
      "import": "./dist/esm/chips.js",
      "default": "./dist/cjs/chips.js"
    },
    "./snapshots": {
      "types": "./dist/esm/snapshots.d.ts",
      "import": "./dist/esm/snapshots.js",
      "default": "./dist/cjs/snapshots.js"
    },
    "./node": {
      "types": "./dist/esm/node.d.ts",
      "import": "./dist/esm/node.js",
      "default": "./dist/cjs/node.js"
    },
    "./epoch": {
      "types": "./dist/esm/epoch.d.ts",
      "import": "./dist/esm/epoch.js",
      "default": "./dist/cjs/epoch.js"
    },
    "./networks": {
      "types": "./dist/esm/networks.d.ts",
      "import": "./dist/esm/networks.js",
      "default": "./dist/cjs/networks.js"
    }
  },
  "files": [
    "README.md",
    "bridge",
    "chips",
    "decentralized",
    "dist",
    "dsl",
    "epoch",
    "networks",
    "node",
    "nta",
    "package.json",
    "rss",
    "snapshots",
    "src",
    "stake"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "concurrently 'tsc -p tsconfig.cjs.json' 'tsc -p tsconfig.esm.json'",
    "dev": "concurrently 'tsc -p tsconfig.cjs.json --watch' 'tsc -p tsconfig.esm.json --watch'",
    "prebuild": "rm -rf dist",
    "download-openapi-json": "tsx scripts/download-openapi-json && biome check --write scripts",
    "generate-openapi-schema": "openapi-typescript scripts/openapi.json -o src/types/openapi-schema.ts && biome check --write src",
    "create-component-aliases": "tsx scripts/create-component-aliases.ts && biome check --write ./",
    "categorize-requests": "tsx scripts/categorize-requests.ts && biome check --write ./"
  },
  "devDependencies": {
    "@types/node": "^20.14.3",
    "map-obj": "^5.0.2",
    "openapi-typescript": "^7.0.0-rc.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "openapi-fetch": "^0.9.7",
    "ts-case-convert": "^2.0.7",
    "type-fest": "^4.20.1"
  }
}
